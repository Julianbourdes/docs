{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Teamova",
  "colors": {
    "primary": "#2563eb",
    "light": "#2563eb",
    "dark": "#3b83f6"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "languages": [
      {
        "language": "en",
        "global": {
          "anchors": [
            {
              "anchor": "Website",
              "icon": "house",
              "href": "https://teamova.net"
            },
            {
              "anchor": "Teamova Application",
              "icon": "gauge",
              "href": "https://app.teamova.net"
            },
            {
              "anchor": "Support",
              "icon": "life-ring",
              "href": "mailto:contact@teamova.net"
            }
          ]
        },
        "tabs": [
          {
            "tab": "Documentation",
            "groups": [
              {
                "group": "Getting Started",
                "pages": [
                  "en/introduction",
                  "en/features"
                ]
              },
              {
                "group": "What's New",
                "pages": [
                  "en/changelog"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "groups": [
              {
                "group": "API Documentation",
                "pages": [
                  "api-reference/introduction"
                ]
              }
            ]
          }
        ]
      },
      {
        "language": "fr",
        "global": {
          "anchors": [
            {
              "anchor": "Site web",
              "icon": "house",
              "href": "https://teamova.net"
            },
            {
              "anchor": "Application Teamova",
              "icon": "gauge",
              "href": "https://app.teamova.net"
            },
            {
              "anchor": "Support",
              "icon": "life-ring",
              "href": "mailto:contact@teamova.net"
            }
          ]
        },
        "tabs": [
          {
            "tab": "Documentation",
            "groups": [
              {
                "group": "Commencer",
                "pages": [
                  "fr/introduction",
                  "fr/features"
                ]
              },
              {
                "group": "Quoi de Neuf",
                "pages": [
                  "fr/changelog"
                ]
              }
            ]
          },
          {
            "tab": "Référence API",
            "groups": [
              {
                "group": "Documentation API",
                "pages": [
                  "api-reference/introduction"
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/teamova-light.svg",
    "dark": "/logo/teamova-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "App Teamova",
        "href": "https://app.teamova.net"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.teamova.net"
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://twitter.com/teamova",
      "website": "https://teamova.net"
    }
  },
  "appearance": {
    "default": "system"
  },
  "fonts": {
    "family": "Inter"
  },
  "banner": {
    "dismissible": false,
    "content": "Application Teamova"
  },
  "icons": {
    "library": "lucide"
  }
}
